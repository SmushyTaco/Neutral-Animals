{
  "schemaVersion": 1,
  "id": "neutral_animals",
  "version": "${version}",
  "name": "Neutral Animals",
  "description": "This mod makes chickens, cows, pigs, rabbits, sheep, and villagers fight back!",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/neutral-animals/issues/",
    "sources": "https://github.com/SmushyTaco/Neutral-Animals/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/neutral_animals/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.neutral_animals.NeutralAnimals"
      }
    ],
    "modmenu": [
      "com.smushytaco.neutral_animals.configuration_support.ModMenuIntegration"
    ]
  },
  "mixins": [
    "neutral_animals.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}",
    "cloth-config2": ">=${clothconfig}"
  },
  "suggests": {
    "modmenu": ">=${modmenu}"
  }
}